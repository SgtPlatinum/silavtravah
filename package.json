{
	"name": "subas",
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"server:build": "tsup",
		"server:dev": "tsup --watch",
		"server:start": "node dist/index.cjs",
		"dev": "vite dev",
		"build": "vite build && pnpm run server:build",
		"preview": "vite preview",
		"start": "node build/index",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"prepare": "pnpm run smui-theme-light && pnpm run smui-theme-dark",
		"smui-theme-light": "smui-theme compile static/smui.css -i src/theme -i ./node_modules",
		"smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark -i ./node_modules"
	},
	"devDependencies": {
		"@fontsource/fira-mono": "^4.5.10",
		"@neoconfetti/svelte": "^1.0.0",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.15.5",
		"@swc/core": "^1.3.50",
		"@types/cookie": "^0.5.1",
		"@types/express": "^4.17.17",
		"@types/faker": "^6.6.9",
		"@types/node": "^18.15.11",
		"@types/nodemailer": "^6.4.7",
		"@typescript-eslint/eslint-plugin": "^5.58.0",
		"@typescript-eslint/parser": "^5.58.0",
		"autoprefixer": "^10.4.14",
		"eslint": "^8.38.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss": "^8.4.21",
		"postcss-import": "^15.1.0",
		"postcss-nested": "^6.0.1",
		"prettier": "^2.8.7",
		"prettier-plugin-svelte": "^2.10.0",
		"sanitize.css": "^13.0.0",
		"svelte": "^3.58.0",
		"svelte-check": "^3.2.0",
		"tailwindcss": "^3.3.1",
		"tslib": "^2.5.0",
		"tsup": "^6.7.0",
		"typescript": "^5.0.4",
		"vite": "^4.2.1"
	},
	"dependencies": {
		"@faker-js/faker": "^7.6.0",
		"@mankins/svelte-adapter-express": "^0.1.2",
		"@material/theme": "^14.0.0",
		"@propero/easy-api": "^2.1.0",
		"@propero/easy-store": "^2.0.0",
		"@propero/security": "^1.0.1",
		"@smui/button": "7.0.0-beta.3",
		"@smui/checkbox": "7.0.0-beta.3",
		"@smui/common": "7.0.0-beta.3",
		"@smui/dialog": "7.0.0-beta.3",
		"@smui/floating-label": "7.0.0-beta.3",
		"@smui/form-field": "7.0.0-beta.3",
		"@smui/icon-button": "7.0.0-beta.3",
		"@smui/line-ripple": "7.0.0-beta.3",
		"@smui/list": "7.0.0-beta.3",
		"@smui/menu": "7.0.0-beta.3",
		"@smui/menu-surface": "7.0.0-beta.3",
		"@smui/notched-outline": "7.0.0-beta.3",
		"@smui/paper": "7.0.0-beta.3",
		"@smui/radio": "7.0.0-beta.3",
