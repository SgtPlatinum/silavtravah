		"@smui/select": "7.0.0-beta.3",
		"@smui/slider": "7.0.0-beta.3",
		"@smui/switch": "7.0.0-beta.3",
		"@smui/tab": "7.0.0-beta.3",
		"@smui/textfield": "7.0.0-beta.3",
		"axios": "^1.3.5",
		"compression": "^1.7.4",
		"connect-typeorm": "^2.0.0",
		"cookie-parser": "^1.4.6",
		"dotenv": "^16.0.3",
		"express": "^4.18.2",
		"express-session": "^1.17.3",
		"leaflet": "^1.9.3",
		"nodemailer": "^6.9.1",
		"pg": "^8.10.0",
		"reflect-metadata": "^0.1.13",
		"smui-theme": "7.0.0-beta.3",
		"typeorm": "^0.3.14",
		"zod": "^3.21.4"
	}
}
